				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 15,
						"startTime": "2024-03-16T16:42:00",
						"endTime": "2024-03-16T17:32:14",
						"timeZone": "India Standard Time"
					}
				}
			},
			"dependsOn": [

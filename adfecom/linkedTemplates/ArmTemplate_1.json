						},
						"parameters": {}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/customer/blobs/",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('SCH_MANNUAL_CSV_PARQUET_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": []
